public class HelloWorld {
	public static void main(String[] args) {
		GTerm gt = new GTerm(1110, 500);
		gt.setFontSize(20);
		gt.setFontColor(0,0,0);
		gt.setXY(100, 0);
		gt.println("Hey! My name is Daniel.");
		gt.setXY(500, 0);
		gt.addImageIcon("Snow.jpg");
		gt.setFontSize(16);
		gt.setXY(70,70);
		gt.println("Here is a picture I took this snow season" + "\n" + "I hope to one day get a Remote work from" + "home job in IT");
		gt.setFontSize(20);
		gt.setXY(200, 115);
		gt.println("AND");
		gt.setFontSize(16);
		gt.setXY(200,150);
		gt.println("I hope to understand how code works.");
		gt.setXY(0,200);
		gt.println("Looking forward to learning alot.");
		
		//gt.showHelp();
	}
}
