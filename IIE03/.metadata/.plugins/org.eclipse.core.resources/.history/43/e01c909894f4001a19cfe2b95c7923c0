import java.util.Scanner;

public class TheBeginning {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter your name");
		String name;
		name=sc.next();

		System.out.print("Hello " + name);
		
		sc.close();
		
;
		//gt.showHelp();
	}
}
